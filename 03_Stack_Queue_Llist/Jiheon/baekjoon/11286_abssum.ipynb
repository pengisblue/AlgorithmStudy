   "source": [
    "from collections import deque\n",
    "\n",
    "T = int(input())\n",
    "dq = deque([])\n",
    "for t in range(T):\n",
    "    x = int(input())\n",
    "    if x != 0:\n",
    "        dq.appendleft(x)"
